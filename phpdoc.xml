<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <parser>
    <target>documentation/php</target>
  </parser>
  <transformer>
    <target>documentation/php</target>
  </transformer>
  <files>
    <directory>app</directory>
    <ignore>database*</ignore>
  </files>
    <logging>
        <level>warn</level>
        <paths>
            <default>logs/phpdocs/{DATE}.log</default>
            <errors>logs/phpdocs/{DATE}.errors.log</errors>
        </paths>
    </logging>
</phpdoc>
